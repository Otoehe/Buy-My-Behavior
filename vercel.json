{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/sw.js",
      "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }]
    },
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|icons|images|sw\\.js|manifest\\.webmanifest).*)",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": false
}
